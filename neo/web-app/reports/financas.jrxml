	<queryString><![CDATA[SELECT pm.data, u.user_real_name, p.nome, p.rg, c.nome as nomeConv, tpm.nome AS nomeProc, tpm.valor_base, pm.valor 
FROM procedimento_medico pm, usuario u,  paciente p, convenio c, tipo_procedimento_medico tpm 
WHERE u.id = pm.medico_id AND tpm.id = pm.tipo_id AND pm.paciente_id = p.id AND p.convenio_id = c.id 
AND (c.id ='$P!{convenioID}%' OR '$P!{convenioID}%' LIKE '%null%')
AND DATE(pm.data) BETWEEN DATE('$P!{dateStart}%') AND DATE('$P!{dateEnd}%')
]]></queryString>

