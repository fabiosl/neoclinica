	<queryString><![CDATA[SELECT idv.quantidade_comprada AS quantidadeComprada, 
c.data_pedido, c.data_recebimento, c.valor, l.tipo, p.nome, p.rg

FROM

item_de_venda idv, compra c, lente l, paciente p 

WHERE idv.compra_id = c.id
AND   idv.lente_id = l.id
AND   c.paciente_id = p.id
AND (DATE_FORMAT(c.data_pedido,'%Y-%m-%d') BETWEEN DATE('$P!{dateStart}%') AND DATE('$P!{dateEnd}%') OR 
     DATE_FORMAT(c.data_recebimento,'%Y-%m-%d') BETWEEN DATE('$P!{dateStart}%') AND DATE('$P!{dateEnd}%')) ORDER BY c.data_recebimento
]]></queryString>

